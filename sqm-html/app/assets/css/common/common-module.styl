//轮播
.slider-wrap{
  width:100%;
  position:relative;
  overflow: hidden;
  white-space: nowrap;
  .slide-list{    
    width:auto;
    overflow:hidden;
    display:inline-block;
    .slide-item{
      display:inline-block;      
      width:315px;
      height:200px;
      position:relative;
      img{
        width:100%;
        height:100%;
      }

      .slide-text{
        height:20px;
        position: absolute;
        bottom:40px;
        padding-left:10px;
        padding-right:10px;
        text-align: center;
      }

      .slide-link{
        display:block;
        height:100%;
      }

    }
  }
  .anchor-list{
    width:100%;
    position: absolute;
    bottom:20px;
    left:0;
    right:0;        
    text-align: center;
    .anchor-item-active{
      background:mc;
      bordre-color:mc;
    }
    .anchor-item{
      &:first-child{
        margin-left:0;
      }
      border-radius:50%;
      display:inline-block;
      width:10px;
      height:10px;
      border:1px solid mcb;      
      margin-left:12px;      
    }
  }

}

