//可输入可选择
.ms-parent {   
  width:auto !important;
  display: block;  
  .ms-choice{
    border-color:mbd;
    height:32px;  
    >span{
        padding-left:10px;
        height: 30px;
      line-height: 30px;
    }
  }

  .ms-drop{
    background: mcb;    
    box-shadow: 0 0 10px 1px ft1;    
    border:none;
    color:ft1;
    ul{
      min-height:240px;
      li{
        &:hover{
          color:mcb;
        }

        label{
          height:30px;
          line-height:30px;
          &:before{
            content: '<i>111</i>',        
            //  &:hover{
            // background-position :0 -23px;
            // }
            display:inline-block;
            width:23px;
            height:23px;
            font-weight:inherit;
            sbg('../../img/check.png',0,0);
            vertical-align middle;
            margin-right :2px;
          }

          input[type="checkbox"]{
            dispaly:none;
          }
          input[type="checkbox"]:checked{
            background-position :0 -48px;
          }
        }   
      }
    }
    .ms-drop input[type="checkbox"] {
      vertical-align: text-top;
      margin-right:5px;
    }
  }

  .ms-search{
    input{
      background-position: 99% -16px;
    }
  } 
  .ms-choice{
    >div{      
       sbg('../../img/arrows-up-down.png',0,0);
      background-size:initial;
      width:10px;
      height:6px;
      display:inline-block;      
      margin-top:14px;
      margin-right:10px;
    }
  }
}

