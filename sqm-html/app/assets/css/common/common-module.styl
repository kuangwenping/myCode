//日期
.calendar{
  //sbg('../../handle-sprite.png',0,0);
  background-size:initial;
}
.calendar-space{
  .calendar-item{
    width:46%;
    display: inline-block;
    vertical-align: middle;
  }
  .interval-text{
    display inline-block;
    width:6%;
    text-align: center;
  }
}
.x-timepicker{
  width:182px;
  height:180px;
  overflow-y:auto;
}
.bui-datepicker{
  z-index :1000;
  position:absolute !important;
  .icon-caret{
    width: 8px;
    height: 8px;
    display: inline-block;
    line-height: 14px;
    sbg('../../img/sprite-469-300.png',0,0);
    background-size:initial;
  }
  .icon-caret-left{
    background-position -1px -227px;
  }
  .icon-caret-right{
    background-position -1px -235px;
  }
  .bui-calendar{
    &:before{
      content: '';
      display: inline-block;
      border-left: 7px solid transparent;
      border-right: 7px solid transparent;
      border-bottom: 7px solid mcb;
      border-top: 0;
      position: absolute;
      left: 5px;
      top: -7px;
    }
    background:mcb;
    box-shadow 0 0 10px 2px ft1;
    padding: 4px 0px;
    margin-top: 4px;
    font-size: 13px;
    border: 1px solid mbd;
    position: relative;
    .bui-calendar-header {
      padding: 0 4px;
      position: relative;
      text-align center;
      .x-datepicker-prev{
        left:4px;

      }
      .x-datepicker-next{
        right:4px;
      }
      .x-datepicker-arrow{
        width: 32px;
        height: 30px;
        line-height: 30px;
        position: absolute;
        top: 0;
        cursor: pointer;
        background-color: #ffffff;
        text-align: center;
      }

      .x-datepicker-month {
        height: 30px;
        line-height: 30px;
        text-align: center;
        cursor: pointer;
        font-weight: bold;
      }
    }
    .bui-calendar-panel {
      padding: 0 4px;
      th {
        padding: 4px 5px;
        width: 22px;
      }

      td.x-datepicker-date{
        text-align center;
        a {
          &:hover{
            background:mtbg;
          }
          display: block;
          padding: 3px 0;
          text-align: center;
          color:ft1;
        }
      }
      td.x-datepicker-prevday{
        a {
          color: ft3;
        }
      }
      td.x-datepicker-selected a {
        &:hover{
          background:aux4;
        }
        color: #ffffff;
        background-color: mc;
        border-radius :50%;
        text-decoration: none;
      }
    }
    .bui-calendar-footer {
      margin-top: 2px;
      padding: 6px 4px 5px;
      border-top: 1px solid #ccc;
      text-align: center;
      .x-datepicker-time{
        width:30%;
